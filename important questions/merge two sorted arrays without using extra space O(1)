#include<bits/stdc++.h>
using namespace std;

void takeinput(vector<int> &v,int n)
{
  for(int i=0;i<n;i++)
  {
    int x;
    cin>>x;
    v.push_back(x);
  }
}

void output(vector<int> v,int n)
{
  for(int i=0;i<n;i++)
  {
    cout<<v[i]<<" ";
  }
}

void merge(vector<int> v1,vector<int> v2,int n,int m)
{
  int i=n-1,j=0;

  while(i>=0 && j<m)
  {
    if(v1[i]>v2[j])
    {
      swap(v1[i],v2[j]);
    }
    i--;
    j++;
  }
  sort(v1.begin(),v1.end());
  sort(v2.begin(),v2.end());


//check kro shi see merge hua hai ki nhi
   output(v1,n); // 0 1 2 3
  cout<<endl;
  output(v2,m);//  5 6 7 8 9

  for(auto it:v2) //v2 par iterate kro kyuki usse jodna hai v1 ke agee it=v2
  {
    v1.push_back(it); // v1 meeh pehle see uska apna data hai 0 1 2 3 abb pushback kro it v2 ko
  }//                                  0 1 2 3  5 6  7 8 9
  output(v1,v1.size());// ans=   0 1 2 3  5 6  7 8 9
} 


int main()
{
  vector<int> v1,v2;

  int n=4;
  int m=5;

  takeinput(v1,n);//1 3 5 7
  takeinput(v2,m);//0 2 6 8 9

  // output(v1,n); 
  // cout<<endl;
  // output(v2,m);
 
  merge(v1,v2,n,m);
  return 0;
}
