--------------------------------------------------------------------------------------------------------------------------------------------------------------
void reverse(queue < int > & q) {
    // Write your code here.
    if(q.size()==0)
    {
        return;
    }
    else{
        int x=q.front();
        q.pop();
        reverse(q);
        q.push(x);
        return;
    }
}

