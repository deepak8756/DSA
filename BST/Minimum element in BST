int minValue(Node* root) {
    // Code here
    if(root==NULL)
    {
       return -1;
    }
    if(root->left==NULL && root->right==NULL || root->left==NULL)
    {
        return root->data;
    }
    
    if(root->left!=NULL)
    {
        minValue(root->left);
    }   
}

