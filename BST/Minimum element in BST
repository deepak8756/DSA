int findPrice(Node* root)
{
    // Write your code here.
    if(root==NULL)
    {
        return -1;
    }
    
    if(root->left==NULL || root->left==NULL && root->right==NULL)
    {
        return root->data;
    }
    if(root->left!=NULL)
    {
        findPrice(root->left);
    }
    else{
        findPrice(root->right);
    }
}
