//Encapsulation

#include<bits/stdc++.h>
using namespace std;

class Hero{
  private:
  string name;  //ye private meeh jo likhe hai ye hai DATA MEMBERS
  int health;
  char level;

  public:
  int gethealth(){   //PUBLIC MEEH JO LIKHE HAI YE HAI FUNCTION 
    return health;    
  }       //         DATA MEMBERS AUR FUNCTION KO CLASS KE ANDAR HAI DONO YE CLASS CAPSULE HO GAI

  void sethealth(int h){
    this->health=h;
  }
};

int main()
{
  Hero h1;
  h1.sethealth(20);
  cout<<h1.gethealth()<<endl;
  return 0;
}

